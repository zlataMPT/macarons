cript>\r\n        window.addEventListener('load', function(){\r\n            var swiper;\r\n\r\n            var testimonials = {\r\n                grid: document.querySelector('.testimonials__grid'),\r\n                items: document.querySelectorAll('.testimonials__item'),\r\n            };\r\n\r\n            function checkAndswitchSlider(){\r\n                var screenWidth = window.screen.width;\r\n\r\n                if( screenWidth < 768 ){\r\n                    testimonials.grid.classList.add('swiper-wrapper');\r\n                    testimonials.items.forEach( function( testimonial ){\r\n                        testimonial.classList.add('swiper-slide');\r\n                    });\r\n\r\n                    swiper = new Swiper('.testimonials__slider', {\r\n                        slidesPerView: 1,\r\n                        spaceBetween: 10,\r\n                        autoplay: {\r\n                          delay: 8000,\r\n                        },\r\n                        pagination: {\r\n                            type: 'bullets',\r\n                            el: '.testimonials__pagination',\r\n                        },\r\n                        autoHeight: true,\r\n                    });\r\n                }\r\n                else{\r\n                    if( swiper ){\r\n                        if(  swiper.destroy ){\r\n                            swiper.destroy(true, true);\r\n                        }\r\n\r\n                        testimonials.grid.classList.remove('swiper-wrapper');\r\n                        testimonials.items.forEach( function( testimonial ){\r\n                            testimonial.classList.remove('swiper-slide');\r\n                        });   \r\n                    }\r\n                }\r\n            }\r\n\r\n            checkAndswitchSlider();\r\n            window.addEventListener('resize', checkAndswitchSlider);\r\n        });\r\n    <\/script>\r\n<\/section><section class=\"partners\">\r\n\t<div class=\"c